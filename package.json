{
  "name": "nextjs-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon --exec next",
    "build": "next build",
    "start": "next start -p $PORT",
    "typecheck": "tsc --noEmit",
    "generate": "graphql-codegen",
    "gen:types": "yarn generate --config codegen.yml",
    "gen:types:watch": "yarn gen:types --watch",
    "precommit": "pretty-quick --staged",
    "codegen-gql": "graphql get-schema && yarn run gen:types"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "aws-amplify": "^1.1.32",
    "dotenv-webpack": "^1.7.0",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "moment": "^2.24.0",
    "next": "^9.1.6",
    "react": "^16.7.0",
    "react-apollo": "^2.5.8",
    "react-apollo-hooks": "^0.5.0",
    "react-dom": "^16.7.0",
    "react-ga": "^2.6.0",
    "react-loading": "^2.0.3",
    "react-toastify": "^5.3.2",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@graphql-codegen/typescript-operations": "^1.4.0",
    "@graphql-codegen/typescript-react-apollo": "^1.4.0",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "babel-plugin-styled-components": "^1.8.0",
    "husky": "^3.0.1",
    "nodemon": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "typescript": "^3.5.3"
  },
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
